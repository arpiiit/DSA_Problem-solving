step-1

temp=[];
for(int i=0;i<n;i++){
    if(arr[i]!=0){
        temp.add(arr[i]);
    }
}

step-2

for(int i=0;i<temp.size();i++){
    arr[i]=temp[i]
}

step-3

noofnonzero=temp.size;

for(int i=noofnonzero;i<n;i++){
    arr[i]=0;
}


